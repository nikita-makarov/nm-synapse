{
	"name": "Dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          vendorID as string,",
				"          tpepPickupDateTime as timestamp,",
				"          tpepDropoffDateTime as timestamp,",
				"          passengerCount as integer,",
				"          tripDistance as double,",
				"          puLocationId as string,",
				"          doLocationId as string,",
				"          startLon as double,",
				"          startLat as double,",
				"          endLon as double,",
				"          endLat as double,",
				"          rateCodeId as integer,",
				"          storeAndFwdFlag as string,",
				"          paymentType as string,",
				"          fareAmount as double,",
				"          extra as double,",
				"          mtaTax as double,",
				"          improvementSurcharge as string,",
				"          tipAmount as double,",
				"          tollsAmount as double,",
				"          totalAmount as double,",
				"          puYear as integer,",
				"          puMonth as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     store: 'synapse',",
				"     databaseType: 'spark',",
				"     format: 'table',",
				"     database: 'default',",
				"     tableName: 'yellow_taxi') ~> source1",
				"source1 select(mapColumn(",
				"          VENDOR_ID = vendorID,",
				"          tpepPickupDateTime,",
				"          tpepDropoffDateTime,",
				"          passengerCount,",
				"          tripDistance,",
				"          fareAmount,",
				"          tipAmount,",
				"          puYear,",
				"          puMonth",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}